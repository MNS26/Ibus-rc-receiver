    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch-serial",
            "type": "mono",
            "request": "launch",
            "args": ["serial","/dev/ttyUSB3"],
            "preLaunchTask": "build console",
            "program": "${workspaceRoot}/RCSerial-Console/bin/Debug/RCSerial-Console.exe",
            "cwd": "${workspaceRoot}",
            "console": "internalConsole"
        },
        {
            "name": "Launch-tcp",
            "type": "mono",
            "request": "launch",
            "args": ["tcp"],
            "preLaunchTask": "build console",
            "program": "${workspaceRoot}/RCSerial-Console/bin/Debug/RCSerial-Console.exe",
            "cwd": "${workspaceRoot}",
            "console": "internalConsole"
        },
        {
            "name": "Launch-udp",
            "type": "mono",
            "request": "launch",
            "args": ["udp"],
            "preLaunchTask": "build console",
            "program": "${workspaceRoot}/RCSerial-Console/bin/Debug/RCSerial-Console.exe",
            "cwd": "${workspaceRoot}",
            "console": "internalConsole"
        },
        {
            "name": "Launch-file",
            "type": "mono",
            "request": "launch",
            "args": ["file"],
            "preLaunchTask": "build console",
            "program": "${workspaceRoot}/RCSerial-Console/bin/Debug/RCSerial-Console.exe",
            "cwd": "${workspaceRoot}",
            "console": "internalConsole"
        },
        {
            "name": "Attach",
            "type": "mono",
            "request": "attach",
            "address": "localhost",
            "port": 55555
        }
    ]
}